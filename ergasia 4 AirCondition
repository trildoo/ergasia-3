class AirCondition {
     private static int length;
     private static int height;
     private static int depth;

     private static String name;
     private static  float price;

    public AirCondition(int length, int height, int depth) {
        this.length = length;
        this.height = height;
        this.depth = depth;
    }
    public AirCondition(String name,float price){
        this.name = name;
        this.price = price;
    }

    public static int getLength() {
        return length;
    }

    public static void setLength(int length) {
        AirCondition.length = length;
    }

    public static int getHeight() {
        return height;
    }

    public static void setHeight(int height) {
        AirCondition.height = height;
    }

    public static int getDepth() {
        return depth;
    }

    public static void setDepth(int depth) {
        AirCondition.depth = depth;
    }

    public static String getName() {
        return name;
    }

    public static void setName(String name) {
        AirCondition.name = name;
    }

    public static float getPrice() {
        return price;
    }

    public static void setPrice(float price) {
        AirCondition.price = price;
    }
    private void functionAC(){

    }
}
